{
  "Infrastructure": {
    "Version": "1.0.0",
    "Compute": {
      "Type": "shared",
      "Configuration": {
        "resourceRequirements": "High",
        "availabilityZone": "us-west-2"
      }
    },
    "Orchestration": {
      "Type": "workflow",
      "Configuration": {
        "workflowEngine": "Abstract Engine",
        "schedule": "Flexible"
      }
    },
    "Storage": {
      "Type": "cloud",
      "Configuration": {
        "storageClass": "Standard",
        "redundancy": "Multi-zone"
      }
    },
    "Persistence": {
      "Type": "cloud",
      "Configuration": {
        "storageType": "parquet",
        "cloudProvider": "AWS",
        "bucketName": "my-bucket",
        "region": "us-west-2",
        "accessKey": "$your-access-key",
        "secretKey": "$your-secret-key"
      }
    },
    "Identity": {
      "Type": "authentication",
      "Configuration": {
        "authService": "OAuth2",
        "authEndpoint": "https://auth.example.com"
      }
    },
    "Environment": {
      "Type": "production",
      "Configuration": {
        "deploymentZone": "us-west-2",
        "scalingPolicy": "auto",
        "environmentVariables": {
          "ENV": "production",
          "DEBUG": "false"
        }
      }
    },
    "Monitoring": {
      "Type": "endpoint",
      "Configuration": {
        "MetricsEndpoint": "http://example.com/metrics",
        "loggingLevel": "INFO",
        "alertingEmail": "alerts@example.com"
      }
    }
  }
}
